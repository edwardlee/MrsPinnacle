{
    "version": "0.1",
    "manifest_version": 2,
    
    "name": "Ms. Everest",
    "description": "First attempt",
    
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "browsingData",
        "tabs"
    ]
}