{
  "name": "Mrs. Pinnacle",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Automate your online lifestyle",
  "icons": {
    "16": "icons/icon-16.png",
    "128": "icons/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/attempt1_background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "scripts/attempt1_content.js",
        "scripts/jquery-3.5.1.min.js",
        "scripts/canvasjs.min.js",
        "scripts/chart.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/icon-19.png",
      "38": "icons/icon-38.png"
    },
    "default_title": "Mrs. Pinnacle",
    "default_popup": "popup.html"
  }
}